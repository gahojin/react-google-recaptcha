{
  "name": "root",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/react-google-recaptcha.git"
  },
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "build": "turbo run build --filter=!./examples/* --filter=!*props-docs",
    "lint": "turbo run lint",
    "check": "turbo run check",
    "check:packagejson": "turbo run check:packagejson",
    "test": "turbo run test:unit --filter=!./examples/* --filter=!*props-docs",
    "clean": "turbo run clean --filter=!./examples/* --filter=!*props-docs",
    "ncu": "npm-check-updates --workspaces -u"
  },
  "dependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.2.2",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@fast-check/vitest": "0.2.2",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react-swc": "4.0.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "jest-extended": "6.0.0",
    "lefthook": "1.12.3",
    "npm-check-updates": "18.0.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "rimraf": "6.0.1",
    "rolldown": "1.0.0-beta.34",
    "terser": "5.43.1",
    "turbo": "2.5.6",
    "typescript": "5.9.2",
    "unplugin-isolated-decl": "0.15.1",
    "vite": "7.1.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=20.x",
    "npm": "use pnpm",
    "pnpm": ">=10.x",
    "yarn": "use pnpm"
  }
}
